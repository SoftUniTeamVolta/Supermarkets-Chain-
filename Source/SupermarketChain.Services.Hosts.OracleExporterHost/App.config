<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
    <add key="LogName" value="OracleExporter"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="OracleExporterTcpBinding"
                 openTimeout="00:30:00"
                 closeTimeout="00:30:00"
                 receiveTimeout="00:30:00"
                 sendTimeout="00:30:00"
                 maxBufferPoolSize="4294967294"
                 maxBufferSize="2147483647"
                 maxConnections="1"
                 maxReceivedMessageSize="4294967294">
          <security mode="None">
            <transport clientCredentialType="None" protectionLevel="None"></transport>
            <message clientCredentialType="None"/>
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="OracleExporterServiceBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetBinding="webHttpBinding" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="SupermaketChain.Services.OracleExporter.OracleExporter"
               behaviorConfiguration="OracleExporterServiceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:12345/" />
            <add baseAddress="net.tcp://localhost:12346/OracleExporter.svc" />
          </baseAddresses>
        </host>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <endpoint address="" binding="netTcpBinding" contract="SupermaketChain.Services.OracleExporter.IOracleExporter" />
      </service>
    </services>
  </system.serviceModel>
</configuration>